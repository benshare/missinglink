{
	"expo": {
		"name": "missinglink",
		"slug": "missinglink",
		"version": "2.0.0",
		"orientation": "portrait",
		"icon": "./assets/images/icon.png",
		"scheme": "missing-link",
		"userInterfaceStyle": "automatic",
		"splash": {
			"image": "./assets/images/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": false,
			"bundleIdentifier": "Missing-Link",
			"buildNumber": "1"
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/adaptive-icon.png",
				"backgroundColor": "#ffffff"
			}
		},
		"web": {
			"bundler": "metro",
			"favicon": "./assets/images/favicon.png"
		},
		"plugins": ["expo-router"],
		"extra": {
			"router": {
				"origin": false
			},
			"eas": {
				"projectId": "fbce50f4-f4e3-4544-895f-099cbf8a7654"
			}
		}
	}
}
